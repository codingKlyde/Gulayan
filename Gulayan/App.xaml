<Application StartupUri="WindowMenu.xaml"
             x:Class="Gulayan.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="Dosis">Assets/Fonts/#Dosis</FontFamily>
            <FontFamily x:Key="Oswald">Assets/Fonts/#Oswald</FontFamily>
            <FontFamily x:Key="Satisfy">Assets/Fonts/#Satisfy</FontFamily>
            <FontFamily x:Key="Ubuntu">Assets/Fonts/#Ubuntu</FontFamily>
           
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/Styles.xaml"/>
                <ResourceDictionary Source="/Styles/WindowLoginStyles.xaml"/>
                <ResourceDictionary Source="/Styles/WindowMenuStyles.xaml"/>
                <ResourceDictionary Source="/Styles/ViewCatalogStyles.xaml"/>
                <ResourceDictionary Source="/Styles/ViewDashboardStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>