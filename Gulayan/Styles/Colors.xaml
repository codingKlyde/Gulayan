<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    
    <!-- Vibrant green for main elements -->
    <SolidColorBrush x:Key="PrimaryColor">#4CAF50</SolidColorBrush>
    <!-- Lighter shade for backgrounds or accents -->
    <SolidColorBrush x:Key="SecondaryColor">#8BC34A</SolidColorBrush>
    <!-- Darker shade for contrast or highlighting -->
    <SolidColorBrush x:Key="TertiaryColor">#388E3C</SolidColorBrush>
    <!-- Complementary shade for buttons or call-to-action elements -->
    <SolidColorBrush x:Key="AccentColor">#CDDC39</SolidColorBrush>
    <!-- For text or subtle background elements -->
    <SolidColorBrush x:Key="NeutralColor">#757575</SolidColorBrush>

    <Color x:Key="Color1">#4CAF50</Color>
    <Color x:Key="Color2">#8BC34A</Color>
    <Color x:Key="Color3">#388E3C</Color>
</ResourceDictionary>