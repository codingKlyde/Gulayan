<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!-- COLOR -->
    <!-- Main color for primary elements -->
    <SolidColorBrush x:Key="PrimaryColor">#6ab04c</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryColorLight">#81d25b</SolidColorBrush>

    <!-- Lighter shade for backgrounds or accents -->
    <SolidColorBrush x:Key="SecondaryColor">#4a372a</SolidColorBrush>
    <SolidColorBrush x:Key="SecondaryColorLight">#5c4434</SolidColorBrush>


    <!-- Darker shade for contrast or highlighting -->
    <SolidColorBrush x:Key="TertiaryColor">#ff7f50</SolidColorBrush>
    <SolidColorBrush x:Key="TertiaryColorLight">#FF9960</SolidColorBrush>

    <!-- Accent color for buttons or call-to-action elements -->
    <SolidColorBrush x:Key="AccentColor">#87ceeb</SolidColorBrush>

    <!-- For text or subtle background elements -->
    <SolidColorBrush x:Key="NeutralColor">#9E9E9E</SolidColorBrush>

    <!-- Color1: Main color for primary elements like controls, text, etc -->
    <Color x:Key="Color1">#6ab04c</Color>

    <!-- Color2: Lighter shade for backgrounds or accents -->
    <Color x:Key="Color2">#4a372a</Color>

    <!-- Color3: Darker shade for contrast or highlighting -->
    <Color x:Key="Color3">#ff7f50</Color>
</ResourceDictionary>